package ro.helator.ie.util;

import javax.xml.parsers.ParserConfigurationException;

import ro.helator.ie.util.xsl.XSLDocument;

public class XSLUtil {

	private static final String XSL_EXTENSION = ".xsl";
	public static XSLDocument createXSLDocument() throws ParserConfigurationException{
		return new XSLDocument("temp");
	}
	
	public static XSLDocument createXSLDocument(String fileName) throws ParserConfigurationException{
		return new XSLDocument(fileName);
	}
}
