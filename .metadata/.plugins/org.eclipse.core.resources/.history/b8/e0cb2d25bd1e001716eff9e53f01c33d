package ro.helator.dia.server;

import java.io.Serializable;

public class Server implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 4026121817394231068L;
	
	private String ip;
	private String port;
	private String user;
	private String password;
	private String karafPath;
	
	public Server(String ip, String port, String user, String password, String karafPath) {
		super();
		this.ip = ip;
		this.port = port;
		this.user = user;
		this.password = password;
		this.karafPath = karafPath;
	}
	
	
}
